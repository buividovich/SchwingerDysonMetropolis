Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"F", "[", 
    RowBox[{"n_", ",", "\[Lambda]_"}], "]"}], ":=", 
   RowBox[{"NIntegrate", "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox["x", 
       RowBox[{"2", "n"}]], 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox["x", "2"]}], "/", "2"}], "-", 
        RowBox[{"\[Lambda]", " ", 
         RowBox[{
          SuperscriptBox["x", "4"], "/", "4"}]}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "\[Infinity]"}], ",", 
       RowBox[{"+", "\[Infinity]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"nmax", "=", "80"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Lambda]0s", "=", 
   RowBox[{"{", 
    RowBox[{
    "0.0", ",", "0.1", ",", "0.2", ",", "0.5", ",", "1.0", ",", "2.0", ",", 
     "5.0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n\[Lambda]", "=", 
   RowBox[{"Length", "[", "\[Lambda]0s", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GrS", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i\[Lambda]", "=", "1"}], ",", 
    RowBox[{"i\[Lambda]", "\[LessEqual]", "n\[Lambda]"}], ",", 
    RowBox[{"i\[Lambda]", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Print", "[", "i\[Lambda]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Lambda]0", "=", 
      RowBox[{"\[Lambda]0s", "[", 
       RowBox[{"[", "i\[Lambda]", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"r", "=", 
