         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"n", "+", "0.5"}], ")"}], " ", 
           RowBox[{"FT1", "[", 
            RowBox[{"[", "n", "]"}], "]"}]}], ")"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "1", ",", "nmax"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Gr", "=", 
      RowBox[{"ListPlot", "[", 
       RowBox[{"FT", ",", 
